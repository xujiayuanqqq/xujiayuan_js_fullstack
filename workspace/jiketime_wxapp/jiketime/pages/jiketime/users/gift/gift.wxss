.hash_head {
    width: 100%;
    height: 200rpx;
    background-color: #fff;
    display: flex;
}
.hash_left {
    width: 50%;
    height: 100%;
    overflow: hidden;
    display: block;
    margin-left: 20rpx;
}
.hash_reward {
    font-size: 35rpx;
    margin-top: 50rpx;
}
.hash_price {
    font-size: 45rpx;
}
.hash_right {
    width: 50%;
    height: 100%;
    overflow: hidden;
    display: block;
}
.hash_detail {
    width: 52rpx;
    height: 40rpx;
    font-size: 26rpx;
    margin-top: 10rpx;
    margin-left: 260rpx;
}
.hash_withdraw {
    font-size: 33rpx;
    color: #E0D6D6;
    text-align: center;
    vertical-align: middle;
    width: 200rpx;
    height: 60rpx;
    background-color: #EEECF7;
    border-radius: 10rpx;
    margin-left: 120rpx;
    margin-top: 30rpx;
}
.hash_body {
    width: 100%;
    height: 350rpx;
    background-color: #fff;
    margin-top: 30rpx;
}
.content {
    width: 100%;
    height: 60rpx;
    font-size: 30rpx;
    text-align: center;
    border-bottom: 1rpx solid #EEEEEE;
}
.condition {
    font-size: 33rpx;
    width: 80%;
    height: 50rpx;
    color: #656363;
    margin-top: 135rpx;
    margin-left: 140rpx;
}
.hash_ft {
    width: 100%;
    position: fixed;
    bottom: 0;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    display: inline-block;
    background: #fff;
}
.hash_rule {
    font-size: 33rpx;
    color: orange;
    text-align: center;
}